{
    "name": "backspace",
    "version": "0.0.4",
    "publisher": "jrieken",
    "description": "âŒ« - erase spaces to the left by multiples of the tab size.",
    "repository": {
        "url": "https://github.com/Microsoft/vscode-backspace.git",
        "type": "git"
    },
    "engines": {
        "vscode": ">=0.9.0-pre.1"
    },
    "main": "./out/extension",
    "activationEvents": [
        "onCommand:backspace++"
    ],
    "contributes": {
        "keybindings": [
            {
                "command": "backspace++",
                "key": "backspace",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
    },
    "devDependencies": {
        "vscode": "next"
    }
}